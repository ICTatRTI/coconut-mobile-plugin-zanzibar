{
  "name": "coconut-mobile-plugin-zanzibar",
  "version": "1.0.0",
  "description": "Plugins added to Coconut Mobile for project specific features",
  "main": "plugin-bundle.js",
  "scripts": {
    "start": "npm run watch & npm run deploy",
    "watch": "./node_modules/watchify/bin/cmd.js --standalone Plugin -v -t coffeeify --extension='.coffee' Plugin.coffee -o plugin-bundle.js",
    "deploy": "./node_modules/chokidar-cli/index.js plugin-bundle.js -c \"/usr/local/bin/couchapp push && echo 'Pushed'\""
  },
  "dependencies": {
    "browserify": "^13.0.1",
    "chance": "^1.0.4",
    "chokidar-cli": "^1.2.0",
    "coffeeify": "^2.0.1",
    "colors": "^1.1.2",
    "underscore": "^1.8.3",
    "watchify": "^3.9.0"
  },
  "devDependencies": {},
  "author": "",
  "license": "ISC"
}
